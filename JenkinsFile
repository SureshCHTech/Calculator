pipeline {
    agent any
    tools {
        maven 'maven'
    }
    stages {
        stage('git_scm') {
            steps {
                git url: "https://github.com/HouariZegai/Calculator.git", branch: "main"
            }
        }
        stage('Build') {
            steps {
                sh 'mvn install -Dmaven.test.skip=true'
            }
        }
    }
}
